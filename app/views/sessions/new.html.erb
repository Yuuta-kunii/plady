<div class="wrapper">
  <h1>log in</h1>
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4">
        <%= form_with url:login_path do |f| %>
        <div class="form-group">
          <%= f.label :email,"メールアドレス",class:"text-white" %>
          <%= f.text_field :email,:placeholder=>"xxxx@xxxx",class:"form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :password,"パスワード",class:"text-white" %>
          <%= f.text_field :password,:placeholder=>"6文字以上32文字以内、小、大英数字含む",class:"form-control" %>
        </div>
          <%= f.submit "log in",class:"btn btn-block btn-submit" %>
        <% end %>  
      </div>
    </div>
  </div>
</div>